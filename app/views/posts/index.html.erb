<div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
<%= link_to 'Add New Post' , new_post_path %>
<% @user.posts.each do |post| %>
<p><%= link_to post.title , post_path(post)%> <%= link_to 'Edit', edit_post_path(post) %> <%= link_to 'Delete', post_path(post), method: :delete %> </p>
<% end %>
</div>